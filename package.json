{
  "name": "line-auth0-link-demo-with-nuxt",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -c auto \"npm:web:dev\" \"npm:web-db:dev\"",
    "preview": "concurrently -c auto \"npm:web:preview\" \"npm:web-db:preview\"",
    "deploy": "concurrently -c auto \"npm:web:deploy\" \"npm:web-db:deploy\"",
    "web:dev": "nuxt dev",
    "web:prod": "nuxt build && nuxt preview",
    "web:build": "nuxt build --preset=cloudflare_pages",
    "web:preview": "bun web:build && wrangler pages dev --port 3000",
    "web:deploy": "bun web:build && wrangler pages deploy",
    "web-db:dev": "bun ./@server/db/scripts/up.sh.ts dev",
    "web-db:prod": "bun ./@server/db/scripts/up.sh.ts prod",
    "web-db:preview": "bun ./@server/db/scripts/up.sh.ts preview",
    "web-db:generate": "drizzle-kit generate --config ./@server/db/config.ts",
    "web-db:migrate": "bun ./@server/db/scripts/migrate.sh.ts",
    "web-db:deploy": "bun ./@server/db/scripts/migrate.sh.ts prod",
    "web-db:studio": "bun ./@server/db/scripts/studio.sh.ts",
    "web-db:clear": "rm -rf ./.wrangler/state/v3/d1",
    "line-messaging": "bun --cwd ./@x-line-messaging --env-file ../.env"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.3",
    "@cloudflare/workers-types": "^4.20240524.0",
    "@line/bot-sdk": "^9.2.2",
    "@line/liff": "^2.23.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@pinia/nuxt": "^0.5.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/bun": "^1.1.3",
    "@vueuse/core": "^10.10.0",
    "@vueuse/nuxt": "^10.10.0",
    "drizzle-orm": "^0.30.10",
    "h3": "^1.11.1",
    "jose": "^5.3.0",
    "messaging-api-line": "^1.1.0",
    "nitro-cloudflare-dev": "^0.1.4",
    "nuxt": "^3.11.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "better-sqlite3": "^10.0.0",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.21.4",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-sort-keys-custom-order": "^1.0.5",
    "eslint-plugin-vue": "^9.26.0",
    "eslint-plugin-vue-scoped-css": "^2.8.0",
    "eslint-plugin-vuejs-accessibility": "^2.3.0",
    "npm-check-updates": "^16.14.20",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "wrangler": "^3.57.2"
  }
}
